{
  "name": "lubycon-vue-ssr-starter",
  "version": "2.0.0",
  "description": "Lubycon, Vue.js + vue-ssr starter kit",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evan-moon/vue-ssr-starter.git"
  },
  "keywords": [
    "vue",
    "ssr"
  ],
  "author": "Evan Moon <bboydart91@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/evan-moon/vue-ssr-starter.git",
  "scripts": {
    "start": "NODE_ENV=production forever start -al forever.log -ao out.log -ae err.log server.js",
    "start:dev": "NODE_ENV=development forever start -al logs/forever.log -ao logs/out.log -ae logs/err.log server.js",
    "start:local": "NODE_ENV=local forever start -al logs/forever.log -ao logs/out.log -ae logs/err.log server.js",
    "stop": "forever stopall",
    "restart": "forever restartall",
    "serve:local": "cross-env NODE_ENV=local node server",
    "serve:dev": "cross-env NODE_ENV=development node server",
    "serve:prod": "cross-env NODE_ENV=production node server",
    "build:client:prod": "cross-env NODE_ENV=production webpack --config ./build/webpack.client.config.js --progress --hide-modules",
    "build:client:dev": "cross-env NODE_ENV=development webpack --config ./build/webpack.client.config.js --progress --hide-modules",
    "build:client:local": "cross-env NODE_ENV=local webpack --config ./build/webpack.client.config.js --progress --hide-modules",
    "build:server:prod": "cross-env NODE_ENV=production webpack --config ./build/webpack.server.config.js --progress --hide-modules",
    "build:server:dev": "cross-env NODE_ENV=development webpack --config ./build/webpack.server.config.js --progress --hide-modules",
    "build:server:local": "cross-env NODE_ENV=local webpack --config ./build/webpack.server.config.js --progress --hide-modules",
    "build": "rimraf dist && npm run build:client:prod && npm run build:server:prod",
    "build:dev": "rimraf dist && npm run build:client:dev && npm run build:server:dev",
    "build:local": "rimraf dist && npm run build:client:local && npm run build:server:local",
    "unit-test": "karma start ./test/unit/karma.conf.js"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^4.0.0-alpha.6",
    "bootstrap-vue": "^1.0.0-beta.9",
    "cookie-parser": "^1.4.3",
    "cross-env": "^3.1.4",
    "express": "^4.14.0",
    "jquery": "^3.2.1",
    "js-cookie": "^2.1.4",
    "lru-cache": "^4.0.1",
    "popper.js": "^1.12.5",
    "q": "^1.5.0",
    "serialize-javascript": "^1.3.0",
    "serve-favicon": "^2.3.0",
    "vee-validate": "^2.0.0-rc.14",
    "vue": "^2.4.2",
    "vue-meta": "^1.2.0",
    "vue-moment": "^2.0.2",
    "vue-no-ssr": "^0.2.0",
    "vue-router": "^2.7.0",
    "vue-server-renderer": "^2.3.4",
    "vuex": "^2.3.0",
    "vuex-router-sync": "^4.1.2"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "babel-core": "^6.25.2",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.13.0",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "eslint": "^3.2.2",
    "eslint-config-standard": "^6.0.0-beta.2",
    "eslint-friendly-formatter": "^2.0.6",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-html": "^1.5.2",
    "eslint-plugin-promise": "^3.3.0",
    "eslint-plugin-standard": "^2.0.0",
    "expose-loader": "^0.7.3",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.8.4",
    "html-webpack-plugin": "^2.28.0",
    "karma": "^1.1.2",
    "karma-mocha": "^1.1.1",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-sinon-chai": "^1.2.3",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^2.0.2",
    "mocha": "^3.0.2",
    "node-sass": "^4.5.3",
    "phantomjs-prebuilt": "^2.1.10",
    "postcss-loader": "^2.0.5",
    "rimraf": "^2.6.0",
    "sass-loader": "^6.0.6",
    "sinon": "^2.1.0",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.19.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.4.0",
    "sw-precache-webpack-plugin": "^0.9.1",
    "url-loader": "^0.5.9",
    "vue-loader": "^12.0.0",
    "vue-ssr-webpack-plugin": "^1.0.2",
    "vue-template-compiler": "^2.4.2",
    "webpack": "^2.3.3",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.2",
    "webpack-merge": "^4.1.0"
  }
}
